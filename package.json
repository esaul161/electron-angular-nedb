{
  "name": "ang-electron",
  "productName": "Punto de Venta",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start-np": "ng serve",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build && electron .",
    "electron-aot": "ng build --prod && electron .",
    "package-win": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages --platform=win32 --arch=all --overwrite",
    "package-all": "npm run build && cross-var electron-packager dist $npm_package_name-$npm_package_version --out=packages  --all --arch=all --overwrite ",
    "electron-package": "ng build --prod && electron-packager .  --icon=dist/assets/icons/png/icono.ico --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --out=packages --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.11",
    "@angular/cdk": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@progress/kendo-angular-buttons": "^4.1.2",
    "@progress/kendo-angular-dateinputs": "^3.4.4",
    "@progress/kendo-angular-dropdowns": "^3.0.2",
    "@progress/kendo-angular-excel-export": "^2.1.1",
    "@progress/kendo-angular-grid": "^3.6.0",
    "@progress/kendo-angular-inputs": "^3.2.0",
    "@progress/kendo-angular-intl": "^1.4.1",
    "@progress/kendo-angular-l10n": "^1.2.0",
    "@progress/kendo-angular-pdf-export": "^1.1.2",
    "@progress/kendo-angular-upload": "^4.1.4",
    "@progress/kendo-data-query": "^1.4.0",
    "@progress/kendo-drawing": "^1.5.7",
    "@progress/kendo-theme-material": "^1.1.4",
    "@types/nedb": "^1.8.5",
    "core-js": "^2.4.1",
    "lowdb": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "nedb": "^1.8.0",
    "ng2-file-upload": "^1.3.0",
    "node-sass": "^4.9.2",
    "rxjs": "^5.5.11",
    "scss-compile": "^0.1.7",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "cross-var": "^1.1.0",
    "electron": "2.0.5",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
